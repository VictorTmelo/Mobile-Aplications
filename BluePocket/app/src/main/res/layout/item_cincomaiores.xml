<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/item_cincomairores_cardview"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorSecondary"
    app:cardBackgroundColor="@color/colorSecondary">

    <TextView
        android:id="@+id/titulo_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:text="Cinco maiores receitas"
        app:layout_constraintEnd_toStartOf="@+id/valor_textView"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/nome_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="14dp"
        android:layout_marginRight="14dp"
        android:text="Nome"
        app:layout_constraintEnd_toStartOf="@+id/titulo_textView"
        app:layout_constraintTop_toBottomOf="@+id/titulo_textView" />

    <TextView
        android:id="@+id/nome2_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:text="Nome"
        app:layout_constraintStart_toStartOf="@+id/nome1_textView"
        app:layout_constraintTop_toBottomOf="@+id/nome1_textView" />

    <TextView
        android:id="@+id/nome3_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:text="Nome"
        app:layout_constraintStart_toStartOf="@+id/nome2_textView"
        app:layout_constraintTop_toBottomOf="@+id/nome2_textView" />

    <TextView
        android:id="@+id/nome4_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Nome"
        app:layout_constraintStart_toStartOf="@+id/nome3_textView"
        app:layout_constraintTop_toBottomOf="@+id/nome3_textView" />

    <TextView
        android:id="@+id/nome1_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Nome"
        app:layout_constraintStart_toStartOf="@+id/nome_textView"
        app:layout_constraintTop_toBottomOf="@+id/nome_textView" />

    <TextView
        android:id="@+id/valor_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:text="Valor"
        app:layout_constraintBottom_toTopOf="@+id/valor1_textView"
        app:layout_constraintStart_toStartOf="@+id/valor1_textView" />

    <TextView
        android:id="@+id/valor1_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:layout_marginBottom="23dp"
        android:text="Valor"
        app:layout_constraintBottom_toTopOf="@+id/valor2_textView"
        app:layout_constraintEnd_toEndOf="@+id/valor2_textView" />

    <TextView
        android:id="@+id/valor2_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="88dp"
        android:layout_marginRight="88dp"
        android:text="Valor"
        app:layout_constraintBottom_toBottomOf="@+id/nome2_textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/nome2_textView" />

    <TextView
        android:id="@+id/valor3_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:text="Valor"
        app:layout_constraintEnd_toEndOf="@+id/valor2_textView"
        app:layout_constraintTop_toBottomOf="@+id/valor2_textView" />

    <TextView
        android:id="@+id/valor4_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:text="Valor"
        app:layout_constraintStart_toStartOf="@+id/valor3_textView"
        app:layout_constraintTop_toBottomOf="@+id/valor3_textView" />
</androidx.constraintlayout.widget.ConstraintLayout>